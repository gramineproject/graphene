loader.preload = "file:../../src/libsysdb.so"
libos.entrypoint = "file:bootstrap"
loader.argv0_override = "bootstrap"

loader.env.LD_LIBRARY_PATH = "/lib:/usr/$(ARCH_LIBDIR)"
loader.debug_type = "file"
loader.debug_file = "tmp/debug_log_file.log"

fs.mount.lib.type = "chroot"
fs.mount.lib.path = "/lib"
fs.mount.lib.uri = "file:../../../../Runtime"

fs.mount.host_usr_lib.type = "chroot"
fs.mount.host_usr_lib.path = "/usr/$(ARCH_LIBDIR)"
fs.mount.host_usr_lib.uri = "file:/usr/$(ARCH_LIBDIR)"

sgx.trusted_files.ld = "file:../../../../Runtime/ld-linux-x86-64.so.2"
sgx.trusted_files.libc = "file:../../../../Runtime/libc.so.6"
sgx.trusted_files.bootstrap = "file:bootstrap"

sgx.nonpie_binary = 1
